# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
charset = utf-8
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[{package.json,.gitlab-ci.yml,.babelrc,.eslitrc}]
charset = utf-8
indent_style = space
indent_size = 2

[*.code-workspace]
charset = utf-8
indent_style = space
indent_size = 2
